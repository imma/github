#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source normalize  

  gh releases "$@" | jq 'map(select(.prerelease != true)) | map({tag: (.tag_name//.name | gsub("^v";"")), assets: (.assets//[] | map(.browser_download_url))}) | .[0]'
}

source sub "$BASH_SOURCE" "$@"
